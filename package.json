{
  "name": "cra-ts",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .ts,.tsx"
    ],
    "*.{js,jsx,,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
